<h1>Friends List</h1>


<% for(var i=0; i < friends.length; i++) { %>
    <li><%= friends[i] %></li>
<% } %>


<form action="/addfriend" method="POST">
    <input type="text" name="newfriend"placeholder="name">
    <button>Make friend</button>
</form>
<p>Total number of friends: <%=friends.length %></p>